@model IEnumerable<System.Web.UI.WebControls.ListItem>
@{
    Layout = null;
}
<table>
    @if (Model.Count() > 0)
    {
        foreach (var member in Model)
        { 
        <tr>
            <td>
                @Html.ActionLink(member.Text, "GetMember", "Home", new { memberID = member.Value }, new { @target = "_blank" })
            </td>
        </tr>
        }
    }
    else
    { 
        <tr>
            <td>
                Every member has a calling.
            </td>
        </tr>
    }
</table>
